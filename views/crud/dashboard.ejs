<!DOCTYPE html>
<html lang="en">
  <%- include("../partials/head.ejs") %>

  <body>
    <%- include("../partials/nav.ejs") %>
    <h1>photoShow</h1>
    <div class="flex-container">
      <% posts.forEach(post => { %>
        <div class="flex-item">
          <a href="/crud/<%=post._id%>"><img src="<%=post.image%>" class="gallery-image" alt=""></a>
          <p class="flex-item-title"><%=post.title%></p>
        </div>
      <% }) %>
    </div>
  </body>
</html>
